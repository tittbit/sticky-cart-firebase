{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/apps/cart-drawer/api/(.*)",
      "destination": "/app/proxy.settings"
    },
    {
      "source": "/cart-drawer.js",
      "destination": "/cart-drawer.js"
    },
    {
      "source": "/(.*)",
      "destination": "/build/index.js"
    }
  ]
}