{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "functions": {
    "build/index.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/apps/cart-drawer/api/(.*)",
      "destination": "/app/proxy.settings"
    },
    {
      "source": "/(.*)",
      "destination": "/build/index.js"
    }
  ]
}